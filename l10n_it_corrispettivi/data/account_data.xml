<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="sequence_corrispettivi_journal" model="ir.sequence">
            <field name="name">Corrispettivi Journal</field>
            <field name="code">account.journal</field>
            <field eval="3" name="padding"/>
            <field name="prefix">COJ/%(year)s/</field>
        </record>
        <record model="account.journal" id="journal_corrispettivi">
            <field name="view_id" ref="account.account_sp_journal_view"/>
            <field name="name">Corrispettivi Journal</field>
            <field name="type">sale</field>
            <field name="code">COJ</field>
            <field name="sequence_id" ref="sequence_corrispettivi_journal"/>
            <field name="analytic_journal_id" ref="account.analytic_journal_sale"/>
            <field name="default_credit_account_id" ref="l10n_it.3101"/>
            <field name="default_debit_account_id" ref="l10n_it.3101"/>
        </record>
        <record model="res.partner" id="partner_corrispettivi">
            <field name="name">Corrispettivi</field>
            <field name="ref">COJ</field>
            <field name="customer" eval="False"></field>
            <field name="supplier" eval="False"></field>
        </record>
        <record model="res.partner.address" id="address_corrispettivi">
            <field name="name">Corrispettivi</field>
            <field name="partner_id" ref="partner_corrispettivi"></field>
        </record>
    </data>
</openerp>
